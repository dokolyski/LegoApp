<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_maxElements2"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:layout_marginTop="24dp"
        android:text="12.12.1212"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/textView_maxElements"
        app:layout_constraintTop_toBottomOf="@+id/textView_down" />

    <TextView
        android:id="@+id/textView_top"
        android:layout_width="248dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:text="Nazwa Projektu"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_down"
        android:layout_width="248dp"
        android:layout_height="31dp"
        android:layout_marginTop="12dp"
        android:text="Kod projektu"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView_top"
        app:layout_constraintTop_toBottomOf="@+id/textView_top" />

    <TextView
        android:id="@+id/textView_maxElements"
        android:layout_width="106dp"
        android:layout_height="24dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="24dp"
        android:text="Last Access:"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_down" />

</androidx.constraintlayout.widget.ConstraintLayout>